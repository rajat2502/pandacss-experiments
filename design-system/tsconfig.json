    "jsx": "react",
    "target": "es2016",
    "module": "NodeNext",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "lib": ["dom", "esnext"],
    "moduleResolution": "NodeNext"
  },
  "include": ["src"]
